@model BlogicRM.Models.Entity.Consultant

@{
    ViewData["Title"] = "Detail poradce";
}

<h1>Detail poradce</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Jméno:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FirstName)
        </dd>
        <dt class="col-sm-2">
            Příjmení:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LastName)
        </dd>
        <dt class="col-sm-2">
            E-mail:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            Telefon:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>
        <dt class="col-sm-2">
            Rodné číslo:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdentificationNumber)
        </dd>
        <dt class="col-sm-2">
            Věk:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Age)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ConsultantID">Upravit</a> |
    <a asp-action="Index">Zpět na seznam poradců</a>
</div>